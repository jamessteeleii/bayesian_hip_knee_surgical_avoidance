  author = {Pedersen, Alma B. and Johnsen, S{\o}ren P. and Overgaard, S{\o}ren and S{\o}balle, Kjeld and S{\o}rensen, Henrik T. and Lucht, Ulf},
  year = 2005,
  month = apr,
  journal = {Acta Orthopaedica},
  volume = {76},
