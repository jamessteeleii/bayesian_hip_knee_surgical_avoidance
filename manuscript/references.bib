@article{mcisaacRelativeEfficacyPrehabilitation2025,
  title = {Relative Efficacy of Prehabilitation Interventions and Their Components: Systematic Review with Network and Component Network Meta-Analyses of Randomised Controlled Trials},
  shorttitle = {Relative Efficacy of Prehabilitation Interventions and Their Components},
  author = {McIsaac, Daniel I. and Kidd, Gurlavine and Gillis, Chelsia and Branje, Karina and {Al-Bayati}, Mariam and Baxi, Adir and Grudzinski, Alexa L. and Boland, Laura and Veroniki, Areti-Angeliki and Wolfe, Dianna and Hutton, Brian},
  year = 2025,
